{
    "name": "alexsoft/cross-origin-protection",
    "description": "Cross Origin Protection middleware that uses Sec-Fetch-Site and Origin headers",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Oleksii Plekhanov",
            "email": "oleksii@p7v.app"
        }
    ],
    "require": {
        "php": "~8.1.0 || ~8.2.0 || ~8.3.0 || ~8.4.0",
        "psr/http-server-middleware": "^1.0"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.48.2",
        "phpstan/phpstan": "^2.1.31",
        "phpunit/phpunit": "^10.5.58",
        "guzzlehttp/psr7": "^2.8"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Alexsoft\\CrossOriginProtection\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        },
        "platform": {
            "php": "8.1.0"
        }
    }
}
